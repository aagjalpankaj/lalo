{
    "name": "aagjalpankaj/laravel-log-validator",
    "description": "Laravel package that helps you to standardize logs",
    "keywords": [
        "laravel",
        "log",
        "validator",
        "standardization",
        "logging"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "monolog/monolog": "^3.9",
        "illuminate/support": "^12.6",
        "illuminate/log": "^12.6",
        "illuminate/config": "^12.6",
        "symfony/error-handler": "^7.2"
    },
    "require-dev": {
        "pestphp/pest": "^3.8",
        "laravel/pint": "^1.21",
        "rector/rector": "^2.0",
        "orchestra/testbench": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Aagjalpankaj\\LaravelLogValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "lint": [
            "@php vendor/bin/pint --ansi",
            "@php vendor/bin/phpstan analyse --verbose --ansi"
        ],
        "test": [
            "@clear",
            "@php vendor/bin/pest"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aagjalpankaj\\LaravelLogValidator\\ServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}